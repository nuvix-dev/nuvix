{
  "name": "nuvix",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nuvix/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "main": "apps/nuvix/src/main.ts",
        "outputPath": "dist/apps/nuvix",
        "outputFileName": "main.js",
        "tsConfig": "apps/nuvix/tsconfig.app.json",
        "rollupConfig": "apps/nuvix/rollup.config.ts",
        "platform": "node",
        "compiler": "swc",
        "format": [
          "esm"
        ],
        "assets": []
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": [
        "build"
      ],
      "options": {
        "buildTarget": "nuvix:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "nuvix:build:development"
        },
        "production": {
          "buildTarget": "nuvix:build:production"
        }
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    }
  }
}